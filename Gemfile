# frozen_string_literal: true
# A sample Gemfile
source "https://rubygems.org"

gem "redcarpet"
gem "jekyll"
gem 'jekyll-sitemap'  # I will generate a sitemap.xml file
                      # I will build my url structure off of the url in your config.yml file
                      # Bonus points - you can notify google of sitemap changes by pinging
                      #https://www.google.com/webmasters/sitemaps/ping?sitemap=http://www.MYSITEURLGOESHERE.com/sitemap.xml